<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 设置图形内的颜色,此处为透明色 -->
    <solid android:color="@color/transparent" />

    <stroke
        android:width="4px"
        android:color="@color/dark_red" />

    <!-- 定义圆角弧度 圆的话就是半径 -->
    <corners android:radius="5dp" />
</shape>
